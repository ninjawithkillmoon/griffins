<%= render 'shared/error_messages', resource: @competition %>

<div class="control-group">
  <%= f.label :name, "Name", class: "control-label" %>
  <div class="controls">
    <%= f.text_field :name %>
  </div>
</div>

<div class="control-group">
  <%= f.label :season_ids, "Seasons", class: "control-label" %>
  <div class="controls">
    <%= f.select :season_ids, Season.all.collect { |comp| [comp.name, comp.id] }, {}, {multiple: true} %>
  </div>
</div>