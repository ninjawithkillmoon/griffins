<% provide(:title, "Edit Season") %> 
<h1>
  <%=@season.name %>
  <small>
    Edit
  </small>
</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@season, html: {class: "form-horizontal well"}) do |f| %>
      
      <%=render 'fields', f: f %>

      <div class="form-actions">
        <%= f.submit "Save changes", class: "btn btn-large btn-success" %>
        <%= link_to 'Cancel', @season, class: "btn btn-large" %>
      </div>
    <% end %>
  </div>
</div>

<% provide :leftNavButtonBody, 'Seasons' %>
<% provide :leftNavButtonUrl, seasons_path %>